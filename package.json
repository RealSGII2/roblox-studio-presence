{
	"private": "true",
	"name": "roblox-studio-presence",
	"version": "1.0.0",
	"description": "A Discord Rich Presence application for Roblox Studio.",
	"main": "src/index.ts",
	"bin": "build/index.js",
	"scripts": {
		"build": "yarn build:rimraf && yarn build:ts && yarn build:pkg && yarn build:cp",
		"build:rimraf": "rimraf build && rimraf bin",
		"build:ts": "tsc",
		"build:pkg": "pkg package.json",
    "build:cp": "cp silent.bat bin/silent.bat",
		"dev": "ts-node ./src/index.ts",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RealSGII2/roblox-studio-presence.git"
	},
	"keywords": [
		"roblox",
		"roblox-studio",
		"discord",
		"discord-rpc",
		"rpc"
	],
	"author": {
		"name": "Thomas Wise",
		"email": "thomas@realsgii2.dev",
		"url": "https://realsgii2.dev/"
	},
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/RealSGII2/roblox-studio-presence/issues"
	},
	"homepage": "https://github.com/RealSGII2/roblox-studio-presence#readme",
	"dependencies": {
		"axios": "^0.24.0",
		"chalk": "^4.1.2",
		"discord-rpc": "^4.0.1",
		"express": "^4.17.1"
	},
	"devDependencies": {
		"@types/discord-rpc": "^4.0.0",
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.10",
		"pkg": "^5.5.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2"
	},
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": ["build/server.asset.js"],
    "outputPath": "bin"
  }
}
